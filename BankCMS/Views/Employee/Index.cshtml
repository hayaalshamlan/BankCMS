@{
}
@model List<Employee>
<h2>List of Employees</h2>
<table class="Table">
    <thead>
        <tr>
            <th>Employee Id</th>
            <th></th>
            <th>FirstName</th>
            <th></th>
            <th>LastName</th>
            <th></th>
            <th>Email</th>
            <th></th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var e in Model)
        {
            <tr>
                <td>@e.EmployeeId</td>
                <td></td>
                <td>@e.FirstName</td>
               <td></td>
                <td>@e.LastName</td>
                <td></td>
                <td>@e.Email</td>
                <td></td>
                <td>@e.Department</td>
            </tr>
        }
    </tbody>
</table>
